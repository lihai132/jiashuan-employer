.app-header {
  position: relative;
}
.banner swiper {
  width: 750rpx;
}
.banner image {
  width: 100%;
  height: 100%;
}
.wx-address-outter {
  position: absolute;
  top: 40rpx;
  left: 30rpx;
}
.wx-address-inner {
  padding: 9rpx 19rpx;
  background: rgba(0, 0, 0, 0.3);
  border-radius: 22rpx;
  font-size: 20rpx;
  color: #fff;
}
.wx-address-inner .arrow-down {
  margin-left: 12rpx;
}
.app-content {
  position: relative;
}
.app-content .home-block {
  position: absolute;
  left: 0;
  width: 750rpx;
  padding-top: 60rpx;
  background: #FFFFFF;
  border-radius: 24rpx 24rpx 0 0;
}
.list-box {
  padding: 0 30rpx;
}
.list-box-item {
  margin-bottom: 60rpx;
}
.wx-title image {
  width: 26rpx;
  height: 16rpx;
}
.wx-title .title {
  margin-left: 20rpx;
  color: #1A1A1A;
  font-size: 36rpx;
  height: 50rpx;
}
.items-wrapper .items {
  position: relative;
  margin-top: 40rpx;
  margin-right: 18rpx;
}
.items-wrapper .items:nth-child(2n) {
  margin-right: 0;
}
.items-wrapper .items .items-img {
  width: 336rpx;
  height: 500rpx;
}
.absolute-block {
  position: absolute;
  top: 0;
  width: 100%;
  text-align: center;
}
.subtitle1 {
  margin-top: 38rpx;
  font-size: 30rpx;
  color: #FF8666;
  height: 40rpx;
  line-height: 40rpx;
}
.subtitle2 {
  margin-top: 300rpx;
  margin-bottom: 12rpx;
  font-size: 32rpx;
  color: #1A1A1A;
  height: 44rpx;
  line-height: 44rpx;
}
.mt264 {
  margin-top: 264rpx;
}
.subtitle3 {
  font-size: 26rpx;
  color: #999;
  height: 36rpx;
  line-height: 36rpx;
}
.no-data {
  width: 750rpx;
}
.no-data-wrap {
  margin-top: 252rpx;
}
.no-data-pic {
  width: 360rpx;
  height: 209rpx;
  overflow: hidden;
}
.no-data-text {
  margin-top: 44rpx;
  margin-bottom: 24rpx;
  color: #666;
  font-size: 28rpx;
}
.btn-primary {
  margin: 0 auto;
  width: 168rpx;
  height: 56rpx;
  line-height: 56rpx;
  background: linear-gradient(90deg, #FFBB9F 0%, #FF8666 100%);
  border-radius: 35px;
  color: #fff;
  font-size: 24rpx;
  text-align: center;
}
.fixed-wrap {
  position: fixed;
  right: 18rpx;
  bottom: 94rpx;
}
.fixed-wrap image {
  width: 156rpx;
  height: 158rpx;
}

/* 拨打电话列表样式 */
.mobile-container {
  height: 92%;
  width: 750rpx;
}
.mobile-list-title {
  height: 50rpx;
  line-height: 50rpx;
  text-align: center;
  color: #1A1A1A;
  font-size: 36rpx;
  margin-top: 50rpx;
  margin-bottom: 40rpx;
}
.mobile-list {
  height: calc(100% - 100rpx);
  padding: 0 30rpx;
  overflow: hidden;
  overflow-y: auto;
}
.mobile-list-item {
  border-top: 2rpx solid #f6f6f6;
  padding: 38rpx 0;
  height: 48rpx;
  line-height: 48rpx;
  font-size: 34rpx;
  color: #FF8666;
}
.mobile-list-item:first-child {
  border-top: none;
}
.mobile-list-item text {
  margin-right: 30rpx;
}