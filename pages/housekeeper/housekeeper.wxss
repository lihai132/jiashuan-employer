.mt60 { margin-top: 60rpx; }
.app-container {
    width: 654rpx;
    margin: 0 48rpx;
}
.personal-left .img-box image {
    width: 210rpx;
    height: 210rpx;
    background: #f6f6f6;
    box-shadow: 0px 42rpx 40rpx -26rpx rgba(155,67,43,0.3);
    border-radius: 10rpx;
    border: 8px solid #f6f6f6;
}
.personal-right {
    margin-left: 40rpx;
}
.personal-right .name {
    height: 60rpx;
    line-height: 60rpx;
    font-size: 44rpx;
    font-weight: bold;
    color: #1d1d1d;
}
.personal-form {
    padding-top: 12rpx;
}
.personal-form-item {
    margin-top: 12rpx;
    font-size: 28rpx;
    line-height: 44rpx;
    color: #333;
}
.personal-form-item .label {
    color: #999;
    margin-right: 10rpx;
}
.img-form-item .title {
    font-size: 32rpx;
    color: #1D1D1D;
    font-weight: 600;
}
.img-form-item .pic {
    margin-top: 40rpx;
}
.img-form-item .pic image {
    width: 654rpx;
    height: 390rpx;
    background: #f6f6f6;
    border-radius: 28rpx;
}
.btn-wrapper {
    margin-top: 76rpx;
    margin-bottom: 16rpx;
}
.btn-wrapper .btn {
    width: 100%;
    border-radius: 50rpx;
    font-weight: 500;
    font-size: 32rpx;
}
.btn-wrapper .btn.btn-primary {
    background-color: #FF8666;
    color: #fff;
}