page {
    background-color: #F6F6F6;
}
.wx-border {
    margin-bottom: 16rpx;
}
.wx-label {
    min-width: 161rpx;
    color: #999999;
    font-size: 29rpx;
}
.wx-txt {
    color: #1D1D1D;
    font-size: 29rpx;
}
.wx-bg-while {
    background: #fff;
}
.wx-container {
    padding: 0 30rpx;
}
/* 服务时间 */ 
.wx-time {
    padding-bottom: 40rpx;
}
.wx-time .text {
    font-size: 28rpx;
    color: #1D1D1D;
    margin-right: 10rpx;
}
.wx-time-info {
    width: 630rpx;
    padding: 30rpx;
    background: #F6F6F6;
}
.wx-time-info .items {
    color: #999;
    font-size: 28rpx;
}
.wx-time-info .items:first-child {
    margin-bottom: 24rpx;
}
.wx-time-info .items .timer {
    color: #1D1D1D;
    font-weight: bold;
}
.wx-time-info .items .timer.on {
    color: #ff8666;
}

/* 服务进度 */
.wx-progress {
    position: relative;
    background-color: #fff;
    padding-bottom: 60rpx;
}
.wx-progress-bg {
    width: 750rpx;
    height: 270rpx;
}
.wx-progress-wrapper {
    position: absolute;
    top: 0;
}
.wx-progress-container {
    padding: 0 30rpx;
    position: relative;
    width: 690rpx;
}
.wx-progress-text {
    margin-top: 80rpx;
    font-size: 36rpx;
    color: #fff;
    font-weight: bold;
}
.wx-progress-text .text2 {
    font-size: 24rpx;
    padding: 5rpx 20rpx;
    border: 1px solid #fff;
    border-radius: 34rpx;
}
.wx-progress-outer {
    width: 690rpx;
    height: 240rpx;
    background-color: #fff;
    box-shadow: 0 4px 20px 0px rgba(0, 0, 0, 0.1);
    border-radius: 16rpx;
    margin-top: 86rpx;
}
.wx-progress-inner {
    padding: 30rpx;
}
.wx-progress-inner .item {
    position: relative;
    width: 198rpx;
    height: 180rpx;
    line-height: 180rpx;
    background: #f6f6f6;
    border-radius: 8rpx;
    color: #ccc;
    text-align: center;
    font-size: 72rpx;
    margin-right: 18rpx;
}
.wx-progress-inner .item2 {
    width: 306rpx;
}
.wx-progress-inner .item:last-child {
    margin-right: 0;
}
.wx-progress-inner .item .wx-tag {
    position: absolute;
    top: 0;
    left: 0;
    width: 100rpx;
    height: 34rpx;
    line-height: 34rpx;
    background: #EEEEEE;
    border-radius: 12rpx 0 12rpx 0;
    font-size: 20rpx;
    text-align: center;
    color: #fff;
    background: linear-gradient(270deg, #777878 0%, #424343 100%);
}
.wx-progress-inner .item .wx-check {
    display: none;
}
.wx-progress-inner .item.start {
    color: #fff;
    background: linear-gradient(226deg, #FFBB9F 0%, #FF8666 100%);
}
.wx-progress-inner .item.start .wx-number {
    color: #fff;
}
.wx-progress-inner .item.finished .wx-check {
    display: block;
    width: 60rpx;
    height: 55rpx;
    position: absolute;
    bottom: 0;
    right: 0;
}
.wx-progress-inner .item.end {
    color: #fff;
    background: linear-gradient(226deg, #FFBB9F 0%, #FF8666 100%);
    opacity: .5;
}
.wx-progress-inner .item.start.end {
    background: linear-gradient(226deg, #FFBB9F 0%, #FF8666 100%);
    opacity: 1;
}
/* 服务情况 */
.wx-long {
    width: 750rpx;
    background-color: #fff;
}
.wx-long-container {
    padding: 30rpx 30rpx 40rpx;
    position: relative;
}
.wx-long-bg image {
    width: 690rpx;
    height: 200rpx;
}
.wx-long-info {
    position: absolute;
    top: 0;
    left: 0;
}
.wx-long-wrapper {
    padding: 30rpx 30rpx 40rpx;
}
.wx-long-wrapper .subtitle {
    font-size: 24rpx;
    line-height: 34rpx;
    color: #fff;
    width: 157rpx;
    height: 50rpx;
}
.wx-long-wrapper .info {
    height: 150rpx;
}
.wx-long-wrapper .info-item {
    width: 230rpx;
    text-align: center;
    color: #fff;
    font-size: 26rpx;
}
.wx-long-wrapper .info-item .order_num {
    font-size: 68rpx;
    line-height: 68rpx;
    font-weight: bold;
}
/* 我的预约 */
.wx-service {
    width: 750rpx;
    background-color: #fff;
}
.wx-service-wrapper {
    padding: 40rpx 0 1rpx 0;
}
.wx-reserve-title .title {
    font-size: 36rpx;
    font-weight: bold;
    line-height: 44rpx;
    color: #1A1A1A;
    width: 100%;
}
.abnormal-text {
    font-size: 28rpx;
    color: #ff8666;
    line-height: 40rpx;
}
.wx-service-info {
    margin-top: 6rpx;
    margin-bottom: 32rpx;
}
.wx-service-item {
    padding-top: 24rpx;
    line-height: 40rpx;
    text-align: left;
    font-size: 28rpx;
    color: #1D1D1D;
}
.wx-service-item .label {
    color: #999;
    width: 160rpx;
    min-width: 160rpx;
}
.btn-more {
    color: #1a1a1a;
    font-size: 29rpx;
    padding: 24rpx 0;
    border-top: 2rpx solid #E6E6E6;
}
.icon-arror {
    margin-left: 22rpx;
    transform: rotate(0deg);
    transition: transform 0.3s;
}
.icon-arror.on {
    transform: rotate(180deg);
}
.icon-arror image {
    width: 16rpx;
    height: 10rpx;
}
/* 家政师信息 */
.wx-housekeeper {
    background-color: #fff;
    width: 100%;
}
.wx-housekeeper-title {
    padding-top: 40rpx;
    padding-bottom: 30rpx;
}
.wx-housekeeper-title .title {
    color: #1A1A1A;
    font-size: 36rpx;
    font-weight: bold;
}
.wx-housekeeper-title image {
    width: 184rpx;
    height: 44rpx;
}
.wx-housekeeper-content {
    padding-bottom: 36rpx;
}
.wx-housekeeper-pic {
    position: relative;
}
.wx-housekeeper-pic image {
    width: 210rpx;
    height: 210rpx;
    margin-right: 24rpx;
    border-radius: 12rpx;
    background-color: #f5f5f5;
}
.houseKeeper-tips {
    position: absolute;
    top: 0;
    left: 0;
    width: 80rpx;
    height: 36rpx;
    line-height: 36rpx;
    border-radius: 12rpx 0 12rpx 0;
    color: #8B7550;
    font-size: 20rpx;
    text-align: center;
    background: #FFDC97;
}
.wx-housekeeper-info {
    width: 100%;
}
.wx-info-left .wx-info-name {
    margin-top: 26rpx;
    color: #1A1A1A;
    font-size: 32rpx;
}
.wx-info-left .wx-info-phone {
    margin-top: 8rpx;
    color: #666;
    font-size: 26rpx;
}
.wx-info-right {
    margin-top: 20rpx;
}
.wx-info-right image {
    width: 84rpx;
    height: 84rpx;
    margin-left: 29rpx;
}
.wx-info-date {
    background: rgba(255, 134, 102, 0.1);
    font-size: 26rpx;
    color: #ff8866;
    margin-top: 40rpx;
    line-height: 36rpx;
    padding: 6rpx 24rpx;
    border-radius: 29rpx;
}

.wx-housekeeper-btn {
    padding-top: 18rpx;
    padding-bottom: 40rpx;
    line-height: 40rpx;
    border-top: 1px solid #e6e6e6;
}
.btn-contact {
    width: 192rpx;
    height: 64rpx;
    line-height: 64rpx;
    margin-left: 30rpx;
    color: #1D1D1D;
    font-size: 29rpx;
    text-align: center;
    border-radius: 32rpx;
    border: 2rpx solid #E5E5E5;
}
.btn-contact.disabled {
    background: #F2F2F2;
    color: #999;
}
/* 订单信息 */
.wx-orderMsg {
    width: 100%;
    padding-bottom: 30rpx;
}
.wx-orderMsg .wx-container {
    background-color: #fff;
}
.wx-title {
    color: #1A1A1A;
    font-size: 36rpx;
    font-weight: bold;
    padding-top: 40rpx;
    padding-bottom: 30rpx;
}
.wx-refund {
    font-size: 24rpx;
    line-height: 34rpx;
    padding: 5rpx 30rpx;
    border: 1px solid #E5E5E5;
    border-radius: 34rpx;
}
.wx-orderMsg-content {
    padding-bottom: 36rpx;
}
.wx-orderMsg-item {
    margin-bottom: 24rpx;
}
/* 按钮组 */
.wx-body .van-button {
    width: 320rpx;
}
.btn-wrapper {
    /* margin-top: 60rpx; */
    position: fixed;
    bottom: 0;
    width: 100%;
    height: 112rpx;
}
.btn-container {
    background-color: #fff;
}
.wx-btn {
    height: 76rpx;
    line-height: 76rpx;
    text-align: center;
    font-size: 32rpx;
    border-radius: 50rpx;
    border: 2rpx solid transparent;
}
.btn-large {
    width: 670rpx;
    margin: 16rpx 40rpx;
}
.btn-mini {
    width: 320rpx;
    margin: 16rpx 40rpx;
}
.btn-default {
    border: 2rpx solid #FF8666;
    color: #ff8866;
}
.btn-primary {
    border: 2rpx solid #FF8666;
    background-color: #FF8666;
    color: #fff;
}
.btn-disabled {
    color: #fff;
    background-color: #FF8666;
    opacity: .5;
    cursor: not-allowed;
}
.wx-fixed {
    position: fixed;
    bottom: 0;
    background: #fff;
}
/* 调查问卷 */
.question-container {
    padding: 0 30rpx;
}
.question-item {
    padding: 40rpx 0;
    border-top: 2rpx solid #f6f6f6;
}
.question-item:first-child {
    border-top: none;
}
.question-label {
    color: #1D1D1D;
    font-size: 32rpx;
}
.select-items {
    width: 88rpx;
    height: 44rpx;
    line-height: 44rpx;
    background-color: #fff;
    color: #666;
    font-size: 29rpx;
    text-align: center;
    border-radius: 29rpx;
    border: 2rpx solid #ddd;
    margin-right: 40rpx;
}
.select-items:last-child {
    margin-right: 0;
}
.select-items.on {
    background-color: #FFF2EF;
    color: #FF8666;
    border: 2rpx solid #FFF2EF;
}
.question-container .wx-btn {
    margin-top: 200rpx;
    margin-bottom: 20rpx;
}
/* 退款 */ 
.popup-box {
    margin-top: 78rpx;
    margin-bottom: 20rpx;
}
.popup-box .item {
    padding: 4rpx 30rpx;
    color: #666;
    font-size: 28rpx;
    line-height: 40rpx;
    margin-right: 40rpx;
    margin-bottom: 40rpx;
    border: 2rpx solid #DDDDDD;
    border-radius: 29rpx;
}
.popup-box .item.on {
    color: #ff8666;
    background: rgba(255, 134, 102, 0.1);
    border: 2rpx solid #ff8666;
}
.popup-textarea .textarea {
    position: relative;
}
.fixed-text {
    position: absolute;
    right: 24rpx;
    bottom: 20rpx;
    color: #999999;
    font-size: 28rpx;
}

/* 更换家政师 */
.popup-container {
    padding: 0 30rpx;
}
.popup-title {
    width: 468rpx;
    color: #1A1A1A;
    font-size: 36rpx;
    line-height: 50rpx;
    font-weight: bold;
    margin: 0 auto;
    text-align: center;
    margin-top: 50rpx;
    margin-bottom: 42rpx;
}
.popup-item {
    height: 124rpx;
}
.popup-item:nth-child(2) {
    border-top: 1px solid #f6f6f6;
    border-bottom: 1px solid #f6f6f6;
}
.popup-item .label, .popup-textarea .label {
    color: #1D1D1D;
    font-size: 32rpx;
    line-height: 44rpx;
    font-weight: 600;
}
.popup-item .items {
    width: 144rpx;
    height: 48rpx;
    line-height: 48rpx;
    color: #666;
    font-size: 28rpx;
    text-align: center;
    border-radius: 29rpx;
    border: 2rpx solid #ddd;
}
.popup-item .items.on {
    color: #ff8666;
    border: 2rpx solid #ff8666;
    background: rgba(255, 134, 102, 0.1);
}
.popup-textarea {
    margin-top: 20rpx;
    padding-bottom: 76rpx;
}
.popup-textarea .label {
    margin-bottom: 30rpx;
}
.popup-textarea textarea {
    padding: 30rpx;
    background: #F2F2F4;
    border-radius: 12rpx;
    font-size: 28rpx;
    line-height: 40rpx;
    min-height: 182rpx;
    color: #999 !important;
}
.popup-btn {
    width: 100%;
    height: 112rpx;
    border-top: 2rpx solid #f6f6f6;
}
.popup-btn .btn {
    width: 690rpx;
    text-align: center;
    color: #fff;
    line-height: 92rpx;
    border-radius: 46rpx;
    background: rgba(255, 134, 102, 0.5);
}
.popup-btn .btn.primary {
    background: rgba(255, 134, 102, 1);
}

/* 生成海报 */
.imagePathBox {
	width: 100%;
	height: 100%;
	background: rgba(0, 0, 0, .7);
	position: fixed;
	top: 0;
	left: 0;
	right: 0;
	bottom: 0;
	z-index: 10;
}
.imagePathBox .icon-close {
    z-index: 11;
    position: absolute;
    top: 5%;
    right: 8%;
}
.imagePathBox .shengcheng {
	width: 90%;
	height: 80%;
	position: fixed;
	top: 50rpx;
	left: 50%;
	margin-left: -45%;
	z-index: 10;
}
.imagePathBox .savePoster {
	display: block;
	width: 670rpx !important;
	height: 104rpx;
	padding: 0;
	line-height: 104rpx;
	text-align: center;
	position: fixed;
	bottom: 50rpx;
    left: 50%;
    transform: translateX(-50%);
	background: linear-gradient(316deg, #F45F3B 0%, #F89554 100%);
	color: #fff;
	font-size: 32rpx;
	border-radius: 12rpx;
}
.imagePathBox .savePoster view {
    height: 104rpx;
}
.imagePathBox .savePoster .icon-download {
    width: 32rpx;
    height: 32rpx;
    margin-right: 14rpx;
}
.imagePathBox .savePoster text {
    height: 44rpx;
    line-height: 44rpx;
}
.van-dialog__button {
    width: 50%;
}
.van-dialog__button button {
	width: 320rpx !important;
}
.van-cell {
    padding: 0 !important;
}