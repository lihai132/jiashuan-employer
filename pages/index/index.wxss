.no-data {
	width: 750rpx;
}
.no-data-wrap {
	margin-top: 252rpx;
}
.no-data-pic {
	width: 360rpx;
	height: 209rpx;
	overflow: hidden;
}
.no-data-text {
	margin-top: 44rpx;
	margin-bottom: 24rpx;
	color: #666;
	font-size: 28rpx;
}
.btn-primary {
	margin: 0 auto;
	width: 168rpx;
	height: 56rpx;
	line-height: 56rpx;
	background: linear-gradient(90deg, #FFBB9F 0%, #FF8666 100%);
	border-radius: 35px;
	color: #fff;
	font-size: 24rpx;
	text-align: center;
}
.fixed-wrap {
	position: fixed;
	right: 18rpx;
	bottom: 94rpx;
}
.fixed-wrap image {
	width: 156rpx;
	height: 158rpx;
}

/* 拨打电话列表样式 */
.mobile-container {
	height: 92%;
	width: 750rpx;
}
.mobile-list-title {
	height: 50rpx;
	line-height: 50rpx;
	text-align: center;
	color: #1A1A1A;
	font-size: 36rpx;
	margin-top: 50rpx;
	margin-bottom: 40rpx;
}
.mobile-list {
	height: calc(100% - 100rpx);
	padding: 0 30rpx;
	overflow: hidden;
	overflow-y: auto;
}
.mobile-list-item {
	border-top: 2rpx solid #f6f6f6;
	padding: 38rpx 0;
	height: 48rpx;
	line-height: 48rpx;
	font-size: 34rpx;
	color: #FF8666;
}
.mobile-list-item:first-child {
	border-top: none;
}
.mobile-list-item text {
	margin-right: 30rpx;
}

.wx-mask {
	z-index: 999;
	width: 100%;
	height: 100%;
	background: rgba(29, 29, 29, 0.5);
	position: fixed;
	top: 0;
	left: 0;
}